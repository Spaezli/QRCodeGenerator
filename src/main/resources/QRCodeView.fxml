                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin></HBox>
                <TableView fx:id="tbl" prefHeight="226.0" prefWidth="600.0">
                    <columns>
                        <TableColumn prefWidth="75.0" text="%col_name" />
                        <TableColumn prefWidth="75.0" text="%col_type" />
                        <TableColumn prefWidth="87.0" text="%col_attr" />
                  <TableColumn prefWidth="87.0" text="%col_qr" />
                    </columns>
                </TableView>
            <Label fx:id="lbl_status" prefWidth="Infinity" text="%lbl_status" />
            </children>
        </VBox>
    </children>
</AnchorPane>
