                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="txf_type" layoutX="10.0" layoutY="10.0" promptText="%txf_type">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="txf_attr" layoutX="20.0" layoutY="20.0" promptText="%txf_attr">
